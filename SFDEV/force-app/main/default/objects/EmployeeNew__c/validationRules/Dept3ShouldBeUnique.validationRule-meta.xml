<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dept3ShouldBeUnique</fullName>
    <active>true</active>
    <description>Dept3 should not be same as Dept1 and Dept2</description>
    <errorConditionFormula>(Department3__c  =  Department1__c ) ||  (Department3__c  =  Department2__c )</errorConditionFormula>
    <errorMessage>Dept3 should be unique</errorMessage>
</ValidationRule>
