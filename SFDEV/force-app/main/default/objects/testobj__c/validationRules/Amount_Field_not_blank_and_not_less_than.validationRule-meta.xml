<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Amount_Field_not_blank_and_not_less_than</fullName>
    <active>true</active>
    <errorConditionFormula>ISBLANK( Amount__c )   ||  Amount__c  &lt;= 100</errorConditionFormula>
    <errorMessage>Amount should not be blank and above 100 rupees</errorMessage>
</ValidationRule>
